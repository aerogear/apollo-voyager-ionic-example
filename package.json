{
  "name": "showcase",
  "version": "1.0.0",
  "private": true,
  "description": "Mono repository for Ionic Showcase",
  "main": "index.js",
  "devDependencies": {
    "del-cli": "3.0.0",
    "@graphback/codegen-offix": "0.11.0-rc5",
    "graphback-cli": "0.11.0-rc5",
    "yarn": "1.22.0"
  },
  "scripts": {
    "start:server": "cd server/ & yarn start",
    "start:client": "cd client/ & yarn start",
    "build:server": "cd server/ && yarn build",
    "build:client": "cd client/ && yarn build",
    "build:clientProd": "cd client/ && yarn build:prod",
    "prepare:client": "del ./client/build ./server/website ; yarn build:clientProd && mv ./client/build/ ./server/website",
    "build": "yarn workspaces run build",
    "unlock": "yarn workspaces run del package-lock.json && del yarn.lock",
    "clean": "yarn workspaces run del ./dist && del ./types",
    "walkthrough": "./scripts/create_walkthrough.sh"
  },
  "workspaces": [
    "client",
    "server"
  ]
}
